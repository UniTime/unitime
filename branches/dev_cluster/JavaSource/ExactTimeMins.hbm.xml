<hibernate-mapping package="org.unitime.timetable.model">
    <class name="ExactTimeMins" table="exact_time_mins">
        <cache include="non-lazy" usage="nonstrict-read-write"/>
        <id
            name="uniqueId"
            column="uniqueid"
            type="java.lang.Long">
            <generator class="org.unitime.commons.hibernate.id.UniqueIdGenerator">
                <param name="sequence">pref_group_seq</param>
            </generator>
        </id>

        <property
            name="minsPerMtgMin"
            column="mins_min"
            type="java.lang.Integer"
            not-null="true"
            length="4"/>

        <property
            name="minsPerMtgMax"
            column="mins_max"
            type="java.lang.Integer"
            not-null="true"
            length="4"/>

        <property
            name="nrSlots"
            column="nr_slots"
            type="java.lang.Integer"
            not-null="true"
            length="4"/>

        <property
            name="breakTime"
            column="break_time"
            type="java.lang.Integer"
            not-null="true"
            length="3"/>

    </class>
</hibernate-mapping>
